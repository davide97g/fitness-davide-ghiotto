{
	"name": "fitness-davide-ghiotto",
	"private": false,
	"version": "0.5.0",
	"type": "module",
	"website": "https://fitness.davideghiotto.it",
	"scripts": {
		"dev": "vite --port 8080",
		"compile": "vue-tsc",
		"build": "vue-tsc && vite build",
		"preview": "vite preview",
		"prepare": "husky install",
		"pwa": "npm run build && npm run pwa:preview",
		"pwa:icons": "npx vue-pwa-asset-generator -a ./src/assets/img/logo.png -o public/img/icons --no-manifest",
		"pwa:preview": "vite --config vite.config.js preview"
	},
	"dependencies": {
		"ant-design-vue": "^3.2.15",
		"apexcharts": "^3.41.1",
		"chart.js": "^4.1.2",
		"firebase": "^9.15.0",
		"pinia": "^2.0.23",
		"sass": "^1.56.0",
		"vue": "^3.2.41",
		"vue-chartjs": "^5.2.0",
		"vue-router": "^4.1.6",
		"vue3-apexcharts": "^1.4.4"
	},
	"devDependencies": {
		"@vitejs/plugin-vue": "^3.2.0",
		"husky": "^8.0.0",
		"typescript": "^4.6.4",
		"unplugin-vue-components": "^0.22.12",
		"vite": "^3.2.3",
		"vite-plugin-pwa": "^0.14.1",
		"vue-tsc": "^1.0.9",
		"workbox-precaching": "^6.5.4"
	}
}
